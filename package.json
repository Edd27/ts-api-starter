{
  "author": "Edgar Benavides",
  "dependencies": {
    "@prisma/client": "5.5.2",
    "cors": "2.8.5",
    "dotenv": "16.3.1",
    "express": "4.18.2",
    "express-validator": "7.0.1",
    "morgan": "1.10.0",
    "socket.io": "4.7.2"
  },
  "description": "This GitHub project template is designed to kickstart your Node.js application development. It provides a set of essential files to help you maintain a clean and organized codebase for your Node.js project.",
  "devDependencies": {
    "@types/cors": "2.8.16",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.7",
    "@types/morgan": "1.9.8",
    "@types/node": "20.8.10",
    "@types/supertest": "2.0.15",
    "@typescript-eslint/eslint-plugin": "6.10.0",
    "@typescript-eslint/parser": "6.10.0",
    "eslint": "8.53.0",
    "eslint-config-prettier": "9.0.0",
    "husky": "9.0.11",
    "jest": "29.7.0",
    "lint-staged": "15.2.2",
    "nodemon": "3.0.1",
    "npm-run-all": "4.1.5",
    "prettier": "3.0.3",
    "prisma": "5.5.2",
    "rimraf": "5.0.5",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "5.2.2"
  },
  "keywords": [
    "typescript",
    "node",
    "eslint",
    "prettier",
    "jest",
    "supertest"
  ],
  "license": "MIT",
  "name": "ts-api-starter",
  "scripts": {
    "build": "npm-run-all format lint test clean && tsc",
    "clean": "rimraf ./build",
    "dev": "nodemon src -e ts,json --exec 'npm run local'",
    "format": "npx prettier --write src",
    "format:check": "npx prettier --check src",
    "lint": "eslint src",
    "local": "ts-node src",
    "prisma:format": "npx prisma format",
    "prisma:migrate:dev": "npx prisma migrate dev",
    "prisma:pull": "npx prisma db pull",
    "prisma:push": "npx prisma db push",
    "prisma:studio": "npx prisma studio",
    "start": "node src",
    "test": "jest",
    "prepare": "husky"
  },
  "version": "1.0.0",
  "lint-staged": {
    "*.{js,ts}": "eslint --cache --fix",
    "*.src": "prettier --write"
  }
}
