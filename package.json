{
  "author": "Edgar Benavides",
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^7.2.0",
    "@prisma/client": "5.21.1",
    "cors": "2.8.5",
    "dotenv": "16.4.5",
    "express": "5.0.1",
    "express-validator": "7.2.0",
    "http-status-codes": "^2.3.0",
    "morgan": "1.10.0",
    "prisma": "5.21.1",
    "socket.io": "4.8.1",
    "swagger-ui-express": "^5.0.1",
    "zod": "^3.23.8"
  },
  "description": "This GitHub project template is designed to kickstart your Node.js application development. It provides a set of essential files to help you maintain a clean and organized codebase for your Node.js project.",
  "devDependencies": {
    "@types/cors": "2.8.17",
    "@types/express": "5.0.0",
    "@types/jest": "29.5.14",
    "@types/morgan": "1.9.9",
    "@types/node": "22.8.0",
    "@types/supertest": "6.0.2",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "8.11.0",
    "@typescript-eslint/parser": "8.11.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "9.1.0",
    "husky": "9.1.6",
    "jest": "29.7.0",
    "lint-staged": "15.2.10",
    "nodemon": "3.1.7",
    "npm-run-all": "4.1.5",
    "prettier": "3.3.3",
    "rimraf": "6.0.1",
    "supertest": "7.0.0",
    "ts-jest": "29.2.5",
    "ts-node": "10.9.2",
    "typescript": "5.6.3"
  },
  "keywords": [
    "typescript",
    "node",
    "eslint",
    "prettier",
    "jest",
    "supertest"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.src": "prettier --write",
    "*.{js,ts}": "eslint --cache --fix"
  },
  "name": "ts-api-starter",
  "scripts": {
    "build": "npm-run-all format lint test clean && tsc",
    "clean": "rimraf ./build",
    "dev": "nodemon src -e ts,json --exec 'npm run local'",
    "format": "npx prettier --write src",
    "format:check": "npx prettier --check src",
    "lint": "eslint src",
    "local": "ts-node src",
    "postinstall": "prisma generate",
    "prepare": "husky",
    "prisma:format": "npx prisma format",
    "prisma:migrate:dev": "npx prisma migrate dev",
    "prisma:pull": "npx prisma db pull",
    "prisma:push": "npx prisma db push",
    "prisma:studio": "npx prisma studio",
    "start": "node src",
    "test": "jest"
  },
  "version": "1.0.0"
}
